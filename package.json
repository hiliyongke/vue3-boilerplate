{
  "name": "vue3",
  "version": "0.0.2",
  "author": "若尘",
  "homepage": "",
  "description": "a seed with vue standard boilerplate project",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build:dev": "vite build --mode development",
    "build:pro": "vite build --mode production",
    "plop": "plop",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "lint:css": "stylelint  src/**/*.{vue,scss,css,sass,less} --fix",
    "prettier": "prettier --write .",
    "deps": "yarn upgrade-interactive --latest",
    "commit": "sh commit.sh",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.27.1",
    "@element-plus/icons-vue": "^1.1.4",
    "@vueuse/components": "^8.5.0",
    "@vueuse/core": "^8.5.0",
    "axios": "^0.27.2",
    "element-plus": "^2.2.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "plop": "^3.1.0",
    "qs": "^6.10.3",
    "vue": "^3.2.34",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.0",
    "@types/node": "^17.0.34",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.4",
    "commitlint": "^17.0.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "import": "^0.0.6",
    "less": "^4.1.2",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.6.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "stylelint": "^14.8.2",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-less": "^1.0.5",
    "stylelint-order": "^5.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.7.2",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-md": "^0.13.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-restart": "^0.1.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^0.34.15",
    "yorkie": "2.0.0",
    "@ls-lint/ls-lint": "^1.11.0",
    "standard-version": "^9.5.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "keywords": [
    "vue"
  ],
  "bugs": {
    "url": "https://xxx/vue/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    "last 3 Chrome versions",
    "last 3 Firefox versions",
    "Safari >= 10",
    "Explorer > 11",
    "Edge >= 12",
    "iOS >= 10",
    "Android >= 6"
  ]
}
